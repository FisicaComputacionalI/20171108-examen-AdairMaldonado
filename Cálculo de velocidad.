#include <iostream>
using namespace std;

int main (){
  float x;
  float t;
  cout<<"Ingrese la distancia en metros"<<endl;
  cin>> x;
  cout<<"Ingrese el tiempo en segundos"<<endl;
  cin>> t;
  cout<<"La velocidad obtenida (en m/s) es: "<< x/t<<endl;
  return 0;
}
